<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %><br>
        <%= @user.created_at %><br>
        <%= @user.updated_at %><br>
        Now: <%= Time.now %>

      </h1>
    </section>
  </aside>
</div>